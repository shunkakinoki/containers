  "repository": {
    "type": "git",
    "url": "https://github.com/shunkakinoki/notebook.git"
  },
  "scripts": {
    "cm": "yarn git-cz",
    "commit": "yarn git-cz",
    "test": "yarn run lint && yarn run release-test",
